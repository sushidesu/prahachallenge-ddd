{
  "name": "prahachallenge-ddd",
  "version": "1.0.0",
  "repository": "git@github.com:sushidesu/prahachallenge-ddd",
  "author": "sushidesu <hugemerge@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev src/main.ts",
    "start": "node dist/main.js",
    "build": "npm-run-all build:clean build:bundle",
    "build:clean": "rimraf dist/*",
    "build:bundle": "esbuild src/main.ts --bundle --platform=node --minify --outdir=dist --format=cjs",
    "lint": "eslint \"src/**\"",
    "lint:fix": "yarn lint --fix",
    "format": "prettier src",
    "format:fix": "prettier --write src",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "2.27.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.4.9",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "esbuild": "^0.12.17",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "prisma": "^2.27.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.1.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  }
}
